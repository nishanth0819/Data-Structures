#include<iostream>
using namespace std;
struct node
{
    int data;
    struct node *next;
}*head=NULL;
void create(int n)
{
    struct node *temp,*p;
    temp=new node;
    cin>>temp->data;
    temp->next=NULL;
    head=temp;
    for(int i=1;i<n;i++)
    {
        int x;
        cin>>x;
        p=new node;
        p->data=x;
        p->next=NULL;
        temp->next=p;
        temp=p;
    }
}
void show(struct node *p)
{ 
    while(p)
    {
        cout<<p->data<<" ";
        p=p->next;
    }
}
int main()
{
    create(5);
    reverse(head);
    show(head);
}
